<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luxiansheng.query">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/button_search"
        android:label="@string/app_name"
        android:supportsRtl="true"
        >
        <activity android:name=".view.Login"  android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ShowScore" android:theme="@style/title"></activity>
        <activity android:name=".view.Main"  android:theme="@style/AppTheme"></activity>
        <activity android:name=".view.ShowInfor" android:theme="@style/title"></activity>
        <activity android:name=".view.Loading" android:theme="@style/loading"  ></activity>
        <activity android:name=".view.Querying" android:theme="@style/loading"  ></activity>
        <activity android:name=".view.DetialsScore" android:theme="@style/title"></activity>
        <activity android:name=".view.Cet" android:theme="@style/title"></activity>
    </application>

</manifest>